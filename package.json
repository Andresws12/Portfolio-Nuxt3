{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "format:check": "prettier --list-different \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "format": "prettier --write \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "lint:check": "eslint --ext .js,.vue,.ts ./",
    "lint": "eslint --ext .js,.vue,.ts --fix ./",
    "build:analyze": "nuxt build --analyze",
    "fix:all": "pnpm format && pnpm lint && pnpm stylelint",
    "check:all": "pnpm format:check && pnpm lint:check && pnpm stylelint:check",
    "stylelint:check": "stylelint \"{,!(node_modules|dist|.output|.nuxt)/**/}*.{css,scss,vue}\"",
    "stylelint": "stylelint --fix \"{,!(node_modules|dist|.output|.nuxt)/**/}*.{css,scss,vue}\""
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.3.0",
    "@nuxtjs/i18n": "8.1.1",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/sitemap": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "nuxt": "^3.10.3",
    "nuxt-delay-hydration": "^1.3.3",
    "nuxt-purgecss": "^2.0.0",
    "nuxt-schema-org": "^3.3.4",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-declaration-strict-value": "^1.10.4",
    "stylelint-scss": "^6.1.0",
    "vue": "^3.4.21",
    "typescript": "^5.3.3",
    "vue-router": "^4.3.0"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "*",
    "node": ">=20"
  }
}
