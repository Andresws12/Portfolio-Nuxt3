{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "format:check": "prettier --list-different \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "format": "prettier --write \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "lint:check": "npx eslint",
    "lint": "npx eslint --fix",
    "build:analyze": "nuxt build --analyze",
    "fix:all": "pnpm format && pnpm lint && pnpm stylelint",
    "check:all": "pnpm format:check && pnpm lint:check && pnpm stylelint:check",
    "stylelint:check": "stylelint \"{,!(node_modules|dist|.output|.nuxt)/**/}*.{css,scss,vue}\"",
    "stylelint": "stylelint --fix \"{,!(node_modules|dist|.output|.nuxt)/**/}*.{css,scss,vue}\"",
    "test:e2e-cy-cli-action": "cypress run --browser chrome",
    "test:e2e-cy": "export NODE_ENV=local && cypress open",
    "test:e2e-cy-cli": "export NODE_ENV=local && cypress run --browser chrome",
    "test:dev:e2e-cy": "export NODE_ENV=development && cypress open",
    "test:dev:e2e-cy-cli": "export NODE_ENV=development && cypress run --browser chrome",
    "test:stg:e2e-cy": "export NODE_ENV=staging && cypress open",
    "test:stg:e2e-cy-cli": "export NODE_ENV=staging && cypress run --browser chrome",
    "test:prod:e2e-cy": "export NODE_ENV=production && cypress open",
    "test:prod:e2e-cy-cli": "export NODE_ENV=production && cypress run --browser chrome"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.0.0-next.1",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^1.1.0",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/i18n": "9.2.1",
    "@nuxtjs/robots": "^5.2.4",
    "@nuxtjs/sitemap": "^7.2.6",
    "@pinia/nuxt": "0.10.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "cypress": "^14.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "nuxt": "^3.15.4",
    "nuxt-delay-hydration": "^1.3.8",
    "nuxt-purgecss": "^2.0.0",
    "nuxt-schema-org": "^4.1.3",
    "pinia": "^3.0.1",
    "prettier": "^3.5.2",
    "sass": "^1.85.1",
    "stylelint": "^16.15.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-declaration-strict-value": "^1.10.10",
    "stylelint-scss": "^6.11.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "*",
    "node": ">=20"
  },
  "dependencies": {
    "eslint-plugin-jest": "^28.11.0"
  }
}
